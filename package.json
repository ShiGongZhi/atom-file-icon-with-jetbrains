{
  "name": "atom-file-icon-with-jetbrains",
  "displayName": "Atom Material Icons + Jetbrains Icons",
  "description": "Atom Material Icons for Visual Studio Code with wider Jetbrains Icons support",
  "version": "0.1.27",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShiGongZhi/atom-file-icon-with-jetbrains"
  },
  "publisher": "SoulFriends",
  "main": "./src/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "iconThemes": [
      {
        "_watch": true,
        "id": "atom-file-icon-with-jetbrains",
        "label": "Atom Material Icons With Jetbrains",
        "path": "./themes/a-wl-file-icon-vscode-icon-theme.json"
      }
    ],
    "configuration": {
      "properties": {
        "atom-file-icon-with-jetbrains.iconpack-angular-old": {
          "type": "boolean",
          "default": false,
          "description": "Use Old Angular Icons"
        },
        "atom-file-icon-with-jetbrains.iconpack-angular-new": {
          "type": "boolean",
          "default": false,
          "description": "Use New Angular Icons"
        },
        "atom-file-icon-with-jetbrains.iconpack-nest": {
          "type": "boolean",
          "default": false,
          "description": "Use NestJS Icons"
        },
        "atom-file-icon-with-jetbrains.iconpack-next": {
          "type": "boolean",
          "default": false,
          "description": "Use Next.js Icons"
        },
        "atom-file-icon-with-jetbrains.iconpack-ngrx": {
          "type": "boolean",
          "default": false,
          "description": "Use NgRx Icons"
        },
        "atom-file-icon-with-jetbrains.iconpack-phalcon": {
          "type": "boolean",
          "default": false,
          "description": "Use Phalcon Icons"
        },
        "atom-file-icon-with-jetbrains.iconpack-rails": {
          "type": "boolean",
          "default": false,
          "description": "Use Rails Icons"
        },
        "atom-file-icon-with-jetbrains.iconpack-jotai": {
          "type": "boolean",
          "default": false,
          "description": "Use Jotai Icons"
        },
        "atom-file-icon-with-jetbrains.iconpack-recoil": {
          "type": "boolean",
          "default": false,
          "description": "Use Recoil Icons"
        },
        "atom-file-icon-with-jetbrains.iconpack-redux": {
          "type": "boolean",
          "default": false,
          "description": "Use Redux Icons"
        },
        "atom-file-icon-with-jetbrains.iconpack-volt": {
          "type": "boolean",
          "default": false,
          "description": "Use Volt Icons"
        },
        "atom-file-icon-with-jetbrains.iconpack-index": {
          "type": "boolean",
          "default": false,
          "description": "Make the file named index have a special icon"
        }
      }
    }
  },
  "engines": {
    "vscode": "^1.21.0"
  },
  "license": "MIT",
  "icon": "logo.png",
  "homepage": "https://github.com/ShiGongZhi/atom-file-icon-with-jetbrains/blob/main/README.md",
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    },
    "virtualWorkspaces": true
  },
  "categories": [
    "Themes"
  ],
  "galleryBanner": {
    "color": "#263238",
    "theme": "dark"
  },
  "keywords": [
    "icons",
    "theme",
    "material",
    "atom",
    "icon-theme",
    "customization",
    "jetbrains",
    "intellij",
    "pycharm",
    "goland",
    "rustrover",
    "webstorm",
    "idea",
    "clion",
    "phpstorm",
    "rubymiine",
    "appcode",
    "datagrip",
    "rider",
    "dataspell",
    "icons",
    "icon-theme",
    "theme",
    "new ui",
    "JetBrains Mono",
    "font",
    "cold",
    "dark",
    "material"
  ]
}
